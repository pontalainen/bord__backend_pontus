{
  "name": "api",
  "type": "module",
  "scripts": {
    "db:controller": "node controller/controller.js",
    "dev": "nodemon controller/controller.js",
    "db:del": "cd db && del themes.db",
    "db:curl": "curl https://media.githubusercontent.com/media/pontalainen/bord__backend_pontus/main/db/themes.db --output db/themes.db",
    "all": "concurrently \"npm run del\" \"npm run curl\" \"npm run controller\"",
    "start": "npm-run-all --parallel db:*"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "concurrently": "^7.6.0",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "node-fetch": "^3.3.0",
    "node.js": "^0.0.1-security",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "sqlite3": "^5.1.2"
  }
}
